echo "restart vanrui-order-app"
#替换源码
cd /data/app-6009-order/
rm -rf conf
rm -rf bin
rm -rf lib
unzip vanrui-order-app*
cp -r /data/app-6009-order/vanrui-order-app-2.1.0-*/*  /data/app-6009-order/
rm -rf /data/app-6009-order/vanrui-order-app-2.1.0-*

#重启
cd /data/script/


unzip /data/fan/vanrui-user-app/target/*.zip
cp -r vanrui-user-app-*/* /data/fan